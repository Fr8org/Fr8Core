<div ng-controller="AccountListController">
    <table datatable="ng" class="row-border hover" dt-options="dtOptionsBuilder">
        <thead>
            <tr>
                <th>ID</th>
                <th>Email Address</th>
                <th>Roles</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="user in ::users" ng-click="openDetails(user)" style="cursor: pointer;">
                <td>{{:: user.id }}</td>
                <td>{{:: user.emailAddress }}</td>
                <td>{{:: user.role }}</td>
            </tr>
        </tbody>
    </table>
</div>
